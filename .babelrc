{
  "presets": ["next/babel"],
  "plugins": [
    "transform-decorators-legacy",
    [
      "import",
      {
        "libraryName": "antd",
        "style": false
      }
    ],
    "react-hot-loader/babel",
    [
      "babel-plugin-styled-components",
      {
        "ssr": true
      }
    ],
    ["styled-components", { "ssr": true, "displayName": true, "preprocess": false }]
  ]
}
